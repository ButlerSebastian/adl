{
  "id": "00000000-0000-0000-0000-000000000001",
  "version": 1,
  "name": "hello_world_agent",
  "description": "Minimal agent that calls a single echo tool.",
  "role": "Demo",
  "llm": "openai",
  "llm_settings": {
    "temperature": 0.2,
    "max_tokens": 512
  },
  "owner": "demo",
  "rag": [],
  "tools": [
    {
      "name": "echo",
      "description": "Echo a string back to the caller.",
      "category": "utility.text.echo",
      "subcategory": "simple",
      "parameters": [
        {
          "name": "text",
          "type": "string",
          "description": "Text to echo.",
          "minLength": 1,
          "required": true
        }
      ],
      "invocation": {
        "type": "python_function",
        "function": "echo"
      },
      "returns": {
        "type": "StringValue",
        "schema": {
          "$ref": "#/$defs/StandardReturnTypes/StringValue"
        },
        "description": "Returns echoed text",
        "examples": [
          {
            "success": true,
            "value": "Hello World"
          }
        ]
      }
    }
  ]
}